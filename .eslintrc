{
	"parser": "babel-eslint",
	"env":{
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules":{
		"no-unused-vars": "off",
		"require-jsdoc": "off",
		"arrow-parents": "off",
		"comma-dangle": "off",
		"operator-linebreak": ["error", "before"],
		"max-len": ["warn", { "code": 120 }]
	},
	"extends": ["eslint:recommended", "google"]
}